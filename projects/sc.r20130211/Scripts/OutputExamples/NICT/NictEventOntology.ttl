# baseURI: http://www.cmusv.edu/SmartCommunity/Scripts/Output/NictEventOntology

@prefix json:    <http://topbraid.org/json#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

json:Event
      rdf:type rdfs:Class ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty json:observation
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:allValuesFrom json:Observation ;
                owl:onProperty json:observation
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty json:_id
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty json:_id
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty json:name
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty json:name
              ] .

json:Observation
      rdf:type rdfs:Class ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:allValuesFrom json:_Who ;
                owl:onProperty json:_who
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:allValuesFrom json:_What ;
                owl:onProperty json:_what
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:allValuesFrom json:_Where ;
                owl:onProperty json:_where
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty json:_what
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty json:_who
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:allValuesFrom json:_When ;
                owl:onProperty json:_when
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty json:_when
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty json:_where
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty json:_members
              ] .

json:_What
      rdf:type rdfs:Class ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:allValuesFrom xsd:decimal ;
                owl:onProperty json:values
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty json:names
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty json:units
              ] .

json:_When
      rdf:type rdfs:Class ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty json:type
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty json:type
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty json:time
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty json:time
              ] .

json:_Where
      rdf:type rdfs:Class ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty json:type
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty json:type
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:allValuesFrom xsd:decimal ;
                owl:onProperty json:coordinates
              ] .

json:_Who
      rdf:type rdfs:Class ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty json:type
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty json:type
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty json:refers
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:allValuesFrom xsd:string ;
                owl:onProperty json:refers
              ] .

json:_id
      rdf:type rdf:Property .

json:_members
      rdf:type rdf:Property .

json:_what
      rdf:type rdf:Property .

json:_when
      rdf:type rdf:Property .

json:_where
      rdf:type rdf:Property .

json:_who
      rdf:type rdf:Property .

json:coordinates
      rdf:type rdf:Property .

json:name
      rdf:type rdf:Property .

json:names
      rdf:type rdf:Property .

json:observation
      rdf:type rdf:Property .

json:refers
      rdf:type rdf:Property .

json:time
      rdf:type rdf:Property .

json:type
      rdf:type rdf:Property .

json:units
      rdf:type rdf:Property .

json:values
      rdf:type rdf:Property .

<http://www.cmusv.edu/SmartCommunity/Scripts/Output/NictEventOntology>
      rdf:type owl:Ontology .
