      owl:onProperty cmusc:hasSensorTypeNames ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom xsd:string ;
      owl:onProperty cmusc:hasVersion ;
    ] ;
